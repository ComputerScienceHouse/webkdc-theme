# Makefile for WebAuth language bindings

srcdir = @srcdir@
top_srcdir = @top_srcdir@
VPATH = @srcdir@

@SET_MAKE@

SUBDIRS = @BINDINGS@

.PHONY: all clean depend install

all: Makefile
	@l='$(SUBDIRS)'; for subdir in $$l; do \
	    echo "making $@ in $$subdir"; \
	    ( cd $$subdir && $(MAKE) $@ ) || exit 1; \
	done

clean depend install:
	@l='$(SUBDIRS)'; for subdir in $$l; do \
	    echo "making $@ in $$subdir"; \
	    ( cd $$subdir && $(MAKE) $@ ) || exit 1; \
	done
